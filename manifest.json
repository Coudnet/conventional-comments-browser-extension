{
  "name": "Conventional comments browser extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An extension for quickly writing conventional comments in GitHub",
  "homepage_url": "https://conventionalcomments.org/",
  "icons": {},
  "default_locale": "en",
  "permissions": ["https://github.com/*"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["src/inject.js"],
      "css": ["src/inject.css"],
      "run_at": "document_idle"
    }
  ]
}
