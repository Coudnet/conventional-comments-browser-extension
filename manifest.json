{
  "name": "Conventional Comments",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "An (unofficial) extension for writing conventional comments in GitHub (very quickly).",
  "homepage_url": "https://conventionalcomments.org/",
  "icons": {

  },
  "default_locale": "en",
  "permissions": ["https://github.com/*"],
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["src/main.js"],
      "css": ["src/main.css"],
      "run_at": "document_idle"
    }
  ]
}
